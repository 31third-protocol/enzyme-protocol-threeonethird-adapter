{
  "name": "@melonproject/utils",
  "version": "2.0.0",
  "description": "Technology Regulated and Operated Investment Funds",
  "author": "Melon Council DAO <team@meloncouncil.io>",
  "license": "GPL-3.0",
  "homepage": "https://melonprotocol.com",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melonproject/protocol.git"
  },
  "bugs": {
    "url": "https://github.com/melonproject/protocol/issues"
  },
  "keywords": [
    "Defi",
    "Ethereum",
    "Melon",
    "Smart Contracts",
    "Solidity"
  ],
  "main": "dist/index.js",
  "module": "dist/utils.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "cache",
    "contracts",
    "artifacts",
    "hardhat.config.js"
  ],
  "scripts": {
    "clean": "rimraf cache artifacts codegen dist",
    "codegen": "crestproject codegen \"artifacts/contracts/**/!(*.dbg).json\" codegen",
    "compile": "CI=true hardhat compile",
    "package": "tsdx build --entry index.ts",
    "build": "yarn compile && yarn codegen && yarn package",
    "test": "TS_JEST_DISABLE_VER_CHECKER=true jest",
    "lint": "yarn lint:prettier && yarn lint:solhint",
    "lint:prettier": "prettier --list-different \"*.{sol,ts}\" \"{tests,contracts,utils}/{*,**/*}.{sol,ts}\"",
    "lint:solhint": "solhint --max-warnings 0 \"contracts/{*,**/*}.sol\"",
    "format": "prettier --write \"*.{sol,ts}\" \"{tests,contracts,utils}/{*,**/*}.{sol,ts}\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@crestproject/crestproject": "^0.0.61",
    "@crestproject/ganache": "^0.0.61",
    "@crestproject/hardhat": "^0.0.61",
    "@openzeppelin/contracts": "^3.2.0",
    "eslint-plugin-prettier": "3.1.4",
    "ethers": "^5.0.19",
    "hardhat": "^2.0.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "rimraf": "^2.7.1",
    "solhint": "^3.2.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "@crestproject/crestproject": "^0.0.60",
    "ethers": "^5.0.18"
  },
  "resolutions": {
    "@typescript-eslint/parser": "^4.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^2.7.1",
    "ts-jest": "^26.4.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3"
  }
}

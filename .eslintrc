{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "plugin:ava/recommended"],
  "plugins": ["flowtype", "ava"],
  "env": {
    "node": true
  },
  "rules": {
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [ "t" ]
      }
    ]
  }
}

{
  "name": "@enzymefinance/codegen",
  "version": "4.0.5",
  "private": true,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"src/**/*.{js,ts}\" --cache",
    "format": "yarn lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ethers": "^5.6.8"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "optionalDependencies": {
    "prettier": "^2.6.1"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": false,
    "splitting": true,
    "clean": true
  },
  "eslintConfig": {
    "extends": "@enzymefinance/eslint-config",
    "rules": {
      "@typescript-eslint/no-shadow": "off"
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  }
}

{
  "name": "@enzymefinance/protocol",
  "version": "4.0.7",
  "description": "Technology Regulated and Operated Investment Funds",
  "author": "Enzyme Council DAO <council@enzyme.finance>",
  "license": "GPL-3.0",
  "homepage": "https://enzyme.finance",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzymefinance/protocol.git"
  },
  "bugs": {
    "url": "https://github.com/enzymefinance/protocol/issues"
  },
  "keywords": [
    "DeFi",
    "Ethereum",
    "Enzyme",
    "Enzyme Finance",
    "Smart Contracts",
    "Solidity"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest --maxWorkers 50%",
    "lint": "eslint \"{deploy,tasks,tests,src}/**/*.{js,ts}\" --cache",
    "format": "pnpm lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@enzymefinance/ethers": "workspace:4.0.7",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "decimal.js": "^10.3.1",
    "ethers": "^5.7.1"
  },
  "devDependencies": {
    "@enzymefinance/eslint-config": "1.0.26",
    "@jest/environment": "^28.1.3",
    "@nomiclabs/hardhat-ethers": "2.1.1",
    "@nomiclabs/hardhat-etherscan": "3.1.0",
    "@openzeppelin-solc-0.7/contracts": "npm:@openzeppelin/contracts@3.4.2-solc-0.7",
    "@openzeppelin/contracts": "3.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@uniswap/v3-periphery": "github:uniswap/v3-periphery#80f26c86c57b8a5e4b913f42844d4c8bd274d058",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hardhat": "2.11.2",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-deploy": "^0.11.15",
    "jest": "^28.1.3",
    "jest-diff": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "jest-esbuild": "^0.2.9",
    "jest-matcher-utils": "^28.1.3",
    "jest-snapshot": "^28.1.3",
    "merkletreejs": "^0.2.31",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^6.2.1",
    "typescript": "^4.5.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": true,
    "clean": true,
    "silent": true,
    "sourcemap": true
  },
  "eslintConfig": {
    "extends": "@enzymefinance/eslint-config",
    "rules": {
      "@typescript-eslint/no-shadow": "off"
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  }
}

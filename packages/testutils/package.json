{
  "name": "@enzymefinance/testutils",
  "version": "4.0.5",
  "private": true,
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint \"src/**/*.{js,ts}\" --cache",
    "format": "pnpm lint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@enzymefinance/ethers": "workspace:4.0.5",
    "@enzymefinance/hardhat": "workspace:4.0.5",
    "@enzymefinance/protocol": "workspace:4.0.5",
    "ethers": "^5.6.8",
    "fs-extra": "^10.1.0",
    "merkletreejs": "^0.2.31"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "eslint": "^8.12.0",
    "tsup": "^6.2.1",
    "typescript": "^4.5.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": false,
    "splitting": true,
    "clean": true,
    "silent": true
  },
  "eslintConfig": {
    "extends": "@enzymefinance/eslint-config",
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    }
  }
}
